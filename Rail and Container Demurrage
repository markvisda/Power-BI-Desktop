    Source = SharePoint.Tables("https://mhxone.sharepoint.com/sites/PlanningTeam", [ApiVersion = 15]),
    #"2d3dfe9d-3743-44db-b326-95fb91c33762" = Source{[Id="2d3dfe9d-3743-44db-b326-95fb91c33762"]}[Items],
    #"Renamed Columns" = Table.RenameColumns(#"2d3dfe9d-3743-44db-b326-95fb91c33762",{{"ID", "ID.1"}}),
    #"Filtered Rows: Date <> null" = Table.SelectRows(#"Renamed Columns", each ([Date] <> null))
in
    #"Filtered Rows: Date <> null"

