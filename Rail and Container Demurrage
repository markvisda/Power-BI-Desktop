1.
Rail Demurrage

let
    Source = SharePoint.Tables("https://mhxone.sharepoint.com/sites/PlanningTeam", [ApiVersion = 15]),
    #"68371188-989a-4112-a4d8-da362c5131a4" = Source{[Id="68371188-989a-4112-a4d8-da362c5131a4"]}[Items],
    #"Renamed Columns" = Table.RenameColumns(#"68371188-989a-4112-a4d8-da362c5131a4",{{"ID", "ID.1"}}),
    #"Added Conditional Column: Facility Revised" = Table.AddColumn(#"Renamed Columns", "Facility Revised", each if Text.Contains([Facility], "Napa") then "Napa" else if Text.Contains([Facility], "Rancho") then "Rancho" else if Text.Contains([Facility], "Stockton") then "Stockton" else if Text.Contains([Facility], "Vernon") then "Vernon" else if Text.Contains([Facility], "Watson") then "Watson" else if Text.Contains([Facility], "Phoenix") then "Phoenix" else "Unclasified"),
    #"Filtered Rows" = Table.SelectRows(#"Added Conditional Column: Facility Revised", each true)
in
    #"Filtered Rows"

